<h1>來這邊新增履歷吧～</h1>

<%= form_with(model: @resume) do |form| %>
  <div>
    <%= form.label :title, "標題" %>
    <%= form.text_field :title, autocomplete: "off", autofocus: true %>
  </div>
  <div>
    <%= form.label :content, "內文" %>
    <%= form.text_area :content %>
  </div>
  <div>
    <%= form.label :status, "狀態" %>
    <%= form.select :status, Resume.all_status %>
  </div>
  
  <%= form.submit %>
<% end %>
